class Solution {
    public boolean isPalindrome(int n) {
    
         if(n<0){
return false; }

        int n1=n;
        int rev=0;
        int rem;

        while(n!=0){
            rem=n%10;
            rev=rev*10+rem;
            n=n/10;
        }
        int n2=rev;

        return n1==n2;
    }
}
